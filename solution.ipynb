    "import glob\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "DATA_PATH = './data'\n",
    "fn = f'{DATA_PATH}/SCE*.csv'\n",
    "\n",
    "dfs=[]\n",
    "\n",
    "for i in glob.glob(fn):\n",
    "    d = pd.read_csv(i, parse_dates=['date'],sep=';')\n",
    "    dfs.append(d)\n",
    "\n",
    "data = pd.concat(dfs)\n",
    "\n",
    "data"
   ]
  }
